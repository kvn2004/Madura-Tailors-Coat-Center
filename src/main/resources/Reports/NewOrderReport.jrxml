	<query language="SQL"><![CDATA[SELECT
    o.OrderID,
    o.CustomerID,
    o.OrderType,
    o.OrderDate,
    o.DueDate,
    o.Design,
    o.Price,
    o.Quantity,
    p.payment_id,
    p.payment_date,
    p.amount,
    p.payment_method,
    p.status
FROM
    orders o
JOIN
    payments p ON o.OrderID = p.order_id
]]></query>
